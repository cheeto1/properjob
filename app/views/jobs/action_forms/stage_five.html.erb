<form action="<%= job_path(@job) %>" method="post">
  <input type="hidden" name="_method" value="patch">
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <% @job.contractor_availabilities.each do |date| %>
    <label for="<%= date.date_available %>"><%= date.date_available %></label>
    <input type="radio" name="date" id="<%= date.date_available %>" value="<%= date.date_available %>">
  <% end %>
  <input type="submit" value="submit">
</form>
